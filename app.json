{
  "expo": {
    "name": "Intercom Talk",
    "slug": "IntercomTalk",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "ios": {
      "bundleIdentifier": "com.blackbean.intercomtalk"
    },
    "android": {
      "package": "com.blackbean.intercomtalk"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
